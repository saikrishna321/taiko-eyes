{
  "name": "taiko-eyes",
  "version": "0.0.3",
  "description": "Applitools Eyes SDK for Taiko.js",
  "main": "./src/index.js",
  "scripts": {
    "test": "npm install -g . && jest __tests__/unit/*.*.js && codecov -F flagname unittests",
    "e2e": "npm install -g . && TAIKO_PLUGIN='taiko-eyes' mocha --recursive --require @babel/register --timeout=99999 __tests__/integration/*.*.js --exit"
  },
  "keywords": [
    "applitools",
    "eyes",
    "taiko",
    "visual testing",
    "testing",
    "taiko-eyes"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saikrishna321/taiko-eyes.git"
  },
  "author": "Sai Krishna <saikrishna321@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saikrishna321/taiko-eyes/issues"
  },
  "dependencies": {
    "@applitools/dom-snapshot": "^3.0.2",
    "@applitools/eyes-common": "^3.9.3",
    "@applitools/eyes-sdk-core": "^5.16.1",
    "@applitools/visual-grid-client": "^12.29.1",
    "lodash": "^4.17.15",
    "chalk": "^2.4.2",
    "rimraf": "^2.7.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/saikrishna321/taiko-eyes",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "@babel/register": "^7.6.0",
    "codecov": "^3.5.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.0.0",
    "jest": "^24.3.1",
    "jest-junit": "^6.3.0",
    "lint-staged": "^9.0.0",
    "mocha": "^6.2.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.10.0",
    "taiko": "git://github.com/getgauge/taiko.git"
  },
  "lint-staged": {
    "**/*.{js}": [
      "prettier --write '**/*.js'",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  }
}
